{
  "name": "koa2-middle",
  "version": "1.2.1",
  "description": "BFF",
  "scripts": {
    "listen": "npx cross-env NODE_ENV=development babel-node dist/bin/www.js",
    "start": "npm run build && cross-env NODE_ENV=development  babel-node dist/bin/www.js",
    "dev": "npx cross-env NODE_ENV=development ./node_modules/.bin/nodemon --exec babel-node src/bin/www.js",
    "build": "npx babel src --out-dir dist --ignore src/views/*.pug",
    "test": "mocha -t 10000",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "commit": "git-cz",
    "release": "standard-version",
    "tag": "git push origin master"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@koa/cors": "^2.2.3",
    "@koa/router": "^8.0.8",
    "app-info-parser": "^1.1.3",
    "axios": "^0.19.2",
    "chai": "^4.3.4",
    "crypto": "^1.0.1",
    "es6-requireindex": "^0.3.10",
    "form-data": "^4.0.0",
    "fs-extra": "^8.1.0",
    "jimp": "^0.16.1",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-static": "^5.0.0",
    "log4js": "^6.2.1",
    "mime-types": "^2.1.31",
    "mocha": "^8.4.0",
    "node-apk": "^1.1.1",
    "qrcode-reader": "^1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "keywords": [
    "node",
    "koa",
    "koa2"
  ]
}
